<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  
  <servlet>
  	<servlet-name>log_in</servlet-name>
  	<servlet-class>com.smha.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>log_in</servlet-name>
  	<url-pattern>/log_in</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>create_user</servlet-name>
  	<servlet-class>com.smha.CreateUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>create_user</servlet-name>
  	<url-pattern>/create_user</url-pattern>
  </servlet-mapping>
</web-app>